	<!-- 分页查询 -->
	<select id="listPage" resultMap="RM_BcsCumi" parameterType="map">
		SELECT <include refid="columns" /> FROM
		(SELECT
		ROWNUMBER() OVER() RN,
		<include refid="columns" />
		FROM BCS_CUMI
		<include refid="findPage_where" />

		<if test="sortColumns != null and sortColumns !=''">
			ORDER BY ${sortColumns}
		</if>) temp
		<where>
			<if test="start != null and end != null">
				temp.RN<![CDATA[> ]]> #{start} and temp.RN <![CDATA[<= ]]> #{end}
			</if>
		</where>
	</select>

	<!-- 查询执行区间记录的指定字段 -->
	<select id="listCumi" resultMap="RM_BcsCumi" parameterType="int">
		SELECT CUST_NO, SHARE_PER_IDNUM,SHARE_HOLD_PERSON,SHARE_PER_IDTY FROM
		(SELECT
		ROWNUMBER() OVER(order by CUST_NO) RN,
		CUST_NO, SHARE_PER_IDNUM,SHARE_HOLD_PERSON,SHARE_PER_IDTY
		FROM BCS_CUMI
		) temp
		<where>
