	<!-- 修改 -->
	<update id="batchUpdateByIds">
		UPDATE BCS_CUMI
	<include refid="update_sql" />
		WHERE
	INST_NOCUST_NO IN
		<foreach item="ids" collection="list" open="(" separator="," close=")">
		#{ids}
		</foreach>
	</update>

	<!-- 根据ID删除 -->
	<delete id="deleteByPrimaryKey">
	<![CDATA[
